  "description": "Turbo快速开发框架",
  "license": "MIT",
  "author": "jiangw1027@gmail.com",
  "keywords": [
    "react",
    "vite",
    "semi-ui",
    "axios",
    "prettier",
    "mobx",
    "formily",
    "storybook"
  ],
  "repository": {
    "type": "git",
